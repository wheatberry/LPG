<h1>Sign In To Add Plates!</h1>

<%= form_tag sessions_path do %>
	<div class="field">
		<%= label_tag :name %>
		<%= text_field_tag :name %>
	</div>
	<div class="field">
		<%= label_tag :password %>
		<%= text_field_tag :password %>
	</div>
	<div class="actions">
		<%= submit_tag "Sign in" %>
	</div>
<% end %>